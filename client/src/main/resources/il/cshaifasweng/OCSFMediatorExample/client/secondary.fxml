<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="il.cshaifasweng.OCSFMediatorExample.client.SecondaryController">
    <children>
        <Label fx:id="gameState" text="O's Turn"/>
        <GridPane alignment="CENTER" gridLinesVisible="true">
            <children>
                <Button fx:id="btn00" onAction="#clicked" GridPane.rowIndex="0" GridPane.columnIndex="0"
                        text=" "/>
                <Button fx:id="btn01" onAction="#clicked" GridPane.rowIndex="0" GridPane.columnIndex="1"
                        text=" "/>
                <Button fx:id="btn02" onAction="#clicked" GridPane.rowIndex="0" GridPane.columnIndex="2"
                        text=" "/>
                <Button fx:id="btn10" onAction="#clicked" GridPane.rowIndex="1" GridPane.columnIndex="0"
                        text=" "/>
                <Button fx:id="btn11" onAction="#clicked" GridPane.rowIndex="1" GridPane.columnIndex="1"
                        text=" "/>
                <Button fx:id="btn12" onAction="#clicked" GridPane.rowIndex="1" GridPane.columnIndex="2"
                        text=" "/>
                <Button fx:id="btn20" onAction="#clicked" GridPane.rowIndex="2" GridPane.columnIndex="0"
                        text=" "/>
                <Button fx:id="btn21" onAction="#clicked" GridPane.rowIndex="2" GridPane.columnIndex="1"
                        text=" "/>
                <Button fx:id="btn22" onAction="#clicked" GridPane.rowIndex="2" GridPane.columnIndex="2"
                        text=" "/>
            </children>
        </GridPane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
